{
  "name": "zhangzhanggui",
  "version": "1.0.0",
  "description": "AI智能记账助手 - 面向理发店个体户的ChatBI应用",
  "main": "expo-router/entry",
  "scripts": {
    "start": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "proxy": "node server/proxy.js",
    "dev": "concurrently \"npm run proxy\" \"npm run web\"",
    "build:android": "eas build --platform android",
    "build:ios": "eas build --platform ios",
    "lint": "eslint .",
    "deploy": "powershell -ExecutionPolicy Bypass -File ./auto-deploy.ps1",
    "deploy:config": "powershell -ExecutionPolicy Bypass -File ./auto-deploy.ps1 -RepoUrl",
    "deploy:force": "powershell -ExecutionPolicy Bypass -File ./auto-deploy.ps1 -Force"
  },
  "dependencies": {
    "@expo/metro-runtime": "~3.2.3",
    "@expo/vector-icons": "^14.0.0",
    "@react-native-async-storage/async-storage": "1.23.1",
    "date-fns": "^3.6.0",
    "date-fns-tz": "^3.1.0",
    "expo": "~51.0.0",
    "expo-av": "~14.0.0",
    "expo-haptics": "~13.0.0",
    "expo-linear-gradient": "~13.0.0",
    "expo-router": "~3.5.0",
    "expo-speech": "~12.0.0",
    "expo-status-bar": "~1.12.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-native": "0.74.1",
    "react-native-chart-kit": "^6.12.0",
    "react-native-gesture-handler": "~2.16.0",
    "react-native-reanimated": "~3.10.0",
    "react-native-safe-area-context": "4.10.1",
    "react-native-screens": "3.31.1",
    "react-native-svg": "15.2.0",
    "react-native-web": "~0.19.10",
    "zustand": "^4.5.0"
  },
  "devDependencies": {
    "@babel/core": "^7.24.0",
    "@types/react": "~18.2.0",
    "concurrently": "^9.2.1",
    "typescript": "~5.3.0"
  },
  "private": true
}
